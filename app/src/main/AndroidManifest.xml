<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tryone">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.MainActivity"
            android:label="@string/app_name"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.Jios.LevelUp.ui.jios.JiosAdder"
            android:label="Jio Adder"
            >
        </activity>

        <activity
            android:name="com.Events.LevelUp.ui.events.EventsAdder"
            android:label="Event Adder"
            >
        </activity>

        <activity
            android:name="com.Mktplace.LevelUp.ui.mktplace.MktplaceAdder"
            android:label="Event Adder"
            >
        </activity>

        <activity
            android:name="com.Events.LevelUp.ui.events.EventPage"
            android:label="Event Page"
            >
        </activity>


    </application>



</manifest>